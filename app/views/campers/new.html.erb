<% if flash[:errors] %>
<% flash[:errors].each do |error| %>
<p style="color:red;"><%= error %></p>
<% end %>
<% end %>

<h1>New Camper Form</h1>

<%= form_for @camper do |f| %>
<%= f.label :name, 'Name:' %>
<%= f.text_field :name %>
<%= f.label :age, 'Age:'%>
<%= f.number_field :age %>

<%= f.submit %>

<% end %>